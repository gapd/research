add: '(000) 000-0000'.	list add: '(00) 0000-0000'.	list add: '000-00'.	list add: '0%'.	list add: '0.00%'.	list add: '0.00E+00'.	^list</body><body package="JaUIPainter" selector="defaultStringFormats">defaultStringFormats	| list |	list := List new.	list add: '(@@@) @@@-@@@@'.	list add: '(@@) @@@@-@@@@'.	list add: '@@@-@@'.	^list</body><body package="JaUIPainter" selector="defaultTimeStampFormats">defaultTimeStampFormats	| list |	list := List new.	list add: 'yyyy/m/d h:mm A/P'.	list add: 'yyyy-m-d h:mm A/P'.	list add: 'yyyy年m月d日 AM/PMh時mm分'.	^list</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it></st-source>